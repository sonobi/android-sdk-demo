<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_banner">

    <fragment
        android:id="@+id/nav_banner"
        android:name="com.sonobi.logos.ui.home.HomeFragment"
        android:label="@string/menu_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/nav_video"
        android:name="com.sonobi.logos.ui.video.VideoFragment"
        android:label="@string/menu_video"
        tools:layout="@layout/fragment_video" />

    <fragment
        android:id="@+id/nav_interstitial"
        android:name="com.sonobi.logos.ui.interstitial.InterstitialFragment"
        android:label="@string/menu_interstitial"
        tools:layout="@layout/fragment_interstitial" />

</navigation>